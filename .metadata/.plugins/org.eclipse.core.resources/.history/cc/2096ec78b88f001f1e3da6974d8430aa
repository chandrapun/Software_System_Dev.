package lab1;

public class Rectangle extends Shape {

	private int height;
	private int weidth;
	
	Rectangle(int sides) {
		super(sides);
		
	}


	@Override
	public int getArea() {
		// TODO Auto-generated method stub
		return 0;
	}


	public int getHeight() {
		return height;
	}


	public void setHeight(int height) {
		this.height = height;
	}


	public int getWeidth() {
		return weidth;
	}


	public void setWeidth(int weidth) {
		this.weidth = weidth;
	}
	
	
}




